# Squaring using Python List
start_time = time.time()
list_square = [x**2 for x in range(100_000_000)]
end_time = time.time()

print(list_square[0: 10])
print(end_time-start_time)


# Squaring using Numpy
start_time = time.time()
arr = np.arange(100_000_000)
list_square = arr**2
end_time = time.time()

print(list_square[0: 10])
print(end_time-start_time)
