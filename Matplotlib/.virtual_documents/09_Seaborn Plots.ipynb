


import seaborn as sns


print(sns.get_dataset_names())


tips = sns.load_dataset("tips")


tips


tips.head()


tips.info()


tips.describe()


sns.lineplot(x="total_bill", y="tip", data=tips)

import matplotlib.pyplot as plt
plt.title("Total bill Vs Tips Plot")
