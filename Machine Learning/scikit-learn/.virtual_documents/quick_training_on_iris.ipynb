


import pandas as pd


from sklearn.ensemble import RandomForestClassifier


data = pd.read_excel("xlsx/iris-train.xlsx")


data


x = data.iloc[:, :-1]


x


y = data.iloc[:, -1]


y


model = RandomForestClassifier()


model.fit(x.values, y.values)


prediction = model.predict([[5, 3.6, 1.4, 0.2]])


prediction


